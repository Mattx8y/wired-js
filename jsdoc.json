{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src/", "src/rest/", "src/gateway/", "src/shards", "package.json", "README.md"]
  },
  "opts": {
    "destination": "./docs/",
    "template": "./node_modules/minami"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  }
}
